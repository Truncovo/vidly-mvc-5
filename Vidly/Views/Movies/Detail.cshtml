@model Vidly.Models.Movie
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<h3>Relese date: @Model.BirthDate</h3>
<h3>Copies: @Model.AvalibleCopies/@Model.TotalCopies</h3>
<h3>Genres: @foreach (var genre in Model.Genres)
            {
                Write(genre.Name);
                Write("  ");
            }</h3>


